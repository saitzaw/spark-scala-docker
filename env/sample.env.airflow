# Airflow configuration 
# PLEASE DON'T USE IN PRODUCTION, USE OTHER MORER SECURE WAY
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflowpass@postgres:5432/airflowdb
AIRFLOW__CELERY__RESULT_BACKEND=db+postgresql://airflow:airflowpass@postgres:5432/airflowdb
AIRFLOW__CELERY__BROKER_URL=redis://redis:6379/0
AIRFLOW__WEBSERVER__EXPOSE_CONFIG=True
AIRFLOW__CORE__HIDE_SENSITIVE_VAR_CONN_FIELDS=True